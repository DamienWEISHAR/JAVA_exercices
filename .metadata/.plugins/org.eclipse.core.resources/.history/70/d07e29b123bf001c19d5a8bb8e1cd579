import entite.Album;
import entite.Artiste;
import entite.Chanson;

// import entite.*; // Tu peux importer tout le contenu du packager Entite avec cette commande

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Artiste a1 = new Artiste();
		a1.setNom("Lavigne");
		a1.setPrenom("Avril");
		a1.setNomDeScene(a1.getPrenom() + " " + a1.getNom());
		System.out.println(a1.getNomDeScene());
		Artiste a2 = new Artiste("Duff", "Hilary","Hilary Duff" ,34 );
		
		System.out.println(a2.getNomDeScene()+ " " + a2.getAge() + " ans!");
		
		Chanson c1 = new Chanson("Titre"); //constructeur avec un paramètre, donc on set par défaut
	
		System.out.println(c1.getTitre());
		c1.setTitre("Complicated");
		System.out.println(c1.getTitre());
		
		Album letGo = new Album();
		letGo.setArtiste(a1);
		
		System.out.println(letGo.getArtiste());
		
		
		
	}

}

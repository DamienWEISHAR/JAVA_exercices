package entite;

public abstract class Sport {
	//ATTRIBUTS
	protected String nom;
	
	//CONSTRUCTEUR
	public Sport() {
		
	}
	public Sport(String nom) {
		
	}
	
	//METHODES
	
	
	//GETTER & SETTER
	public String getNom() {	
		return this.nom;
	}
	
	public void setNom(String nom) {
		this.nom = nom;
	}
	
	
}

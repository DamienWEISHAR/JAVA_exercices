package entite;

import java.util.ArrayList;
import java.util.List;

public class Parking {
	//ATTRIBUTS
	public String nom;
	public List<Vehicule> listeRoulantAutorise;
	
	//CONSTRUCTEUR
	public Parking() {
		this.init();
	}
	
	public Parking(String nom) {
		this.setNom(nom);
		this.init();
	}

	//METHODES
	
	public void init() {
		this.listeRoulantAutorise = new ArrayList<>();
	}
	
	public void ajoutRoulant(Vehicule roulant) {
		
	}
	
	
	//GETTER & SETTER
	public String getNom() {
		return nom;
	}

	public void setNom(String nom) {
		this.nom = nom;
	}

	public List<Roulant> getListeVehiculeAutorise() {
		return listeRoulantAutorise;
	}

	public void setListeVehiculeAutorise(List<Roulant> listeVehiculeAutorise) {
		this.listeRoulantAutorise = listeVehiculeAutorise;
	}
	
	
	

}
